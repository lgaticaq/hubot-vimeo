{
  "name": "hubot-vimeo",
  "version": "2.0.1",
  "description": "A hubot script for searching Vimeo",
  "main": "src",
  "scripts": {
    "lint": "eslint . --fix",
    "format": "prettier-standard 'src/**/*.js' 'test/**/*.js'",
    "precommit": "lint-staged",
    "pretest": "npm run lint -s && npm run format -s",
    "test": "istanbul cover _mocha"
  },
  "engines": {
    "node": ">=8"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/lgaticaq/hubot-vimeo.git"
  },
  "keywords": [
    "hubot",
    "hubot-scripts",
    "vimeo"
  ],
  "author": "Leonardo Gatica <lgatica@protonmail.com> (https://about.me/lgatica)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lgaticaq/hubot-vimeo/issues"
  },
  "homepage": "https://github.com/lgaticaq/hubot-vimeo#readme",
  "dependencies": {
    "vimeo": "2.1.0"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "coffee-script": "1.12.7",
    "eslint": "5.6.1",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-node": "7.0.1",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-standard": "4.0.0",
    "hubot-test-helper": "1.9.0",
    "husky": "1.0.1",
    "istanbul": "0.4.5",
    "lint-staged": "7.3.0",
    "mocha": "5.2.0",
    "prettier-standard": "8.0.1",
    "proxyquire": "2.1.0"
  },
  "eslintConfig": {
    "extends": "standard",
    "rules": {
      "no-console": [
        "error"
      ]
    }
  },
  "eslintIgnore": [
    "coverage"
  ],
  "lint-staged": {
    "src/**/*.js": [
      "eslint --fix",
      "prettier-standard",
      "git add"
    ],
    "test/**/*.js": [
      "eslint --fix",
      "prettier-standard",
      "git add"
    ]
  },
  "renovate": {
    "automerge": "minor",
    "extends": [
      ":library"
    ]
  }
}
